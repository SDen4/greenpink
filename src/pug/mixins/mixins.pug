mixin menu(className, items)
    each item in items
        a(class=className)= item


mixin smoothBlocks(className, classNameBlock, classNameWrapper, classNamePic, items)
    div(class=className)
        each item in items
            div(class=classNameBlock)
                div(class=classNameWrapper)
                    img(class=classNamePic src=item)


mixin bouquets(className, classNamePic, items)
    each item in items
        div(class=className)
            img(class=classNamePic src=item)